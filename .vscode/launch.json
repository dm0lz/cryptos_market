      "pathToBundler": "/Users/olivier/.rvm/gems/ruby-2.5.3/bin/bundle",
      "pathToRDebugIDE": "/Users/olivier/.rvm/gems/ruby-2.5.3/gems/ruby-debug-ide-0.6.1",
      "program": "${workspaceRoot}/bin/rails",
      "args": ["server", "-p", "3000"]
    }
  ]
}
