  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start Rails server",
      "type": "Ruby",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "program": "${workspaceRoot}/bin/rails",
      "args": ["server", "-p", "3000"]
    },
    {
      "name": "Debug Rails server",
      "type": "Ruby",
